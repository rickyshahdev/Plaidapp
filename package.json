{
  "name": "mern-auth",
  "version": "1.0.0",
  "description": "Mern Auth Example",
  "main": "server.js",
  "engine": {
    "node": "12.18.3",
    "npm": "6.14.6"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
