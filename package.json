    "express": "^4.16.4",
    "http-proxy-middleware": "^1.0.6",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.29.1",
    "mongoose": "^5.3.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "plaid": "^8.1.6",
    "react-datetime-picker": "^3.1.0",
    "validator": "^10.9.0"
