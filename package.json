{
  "name": "mern-auth",
  "version": "1.0.0",
  "description": "Mern Auth Example",
  "main": "server.js",
  "proxy": "http://localhost:5000",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=true npm install -- prefix client && npm run build --prefix client"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jose": "^3.7.0",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "mongoose": "^5.3.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "plaid": "^8.2.0",
    "react-datetime-picker": "^3.1.0",
    "secure-compare": "^3.0.1",
    "validator": "^10.9.0"
  }
}
